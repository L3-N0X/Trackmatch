{
  "name": "new-ts",
  "version": "1.0.1",
  "private": true,
  "main": "electron-main.js",
  "homepage": "./",
  "description": "Match your DJ library with Spotify and create playlists with ease.",
  "author": "L3-N0X",
  "keywords": [
    "spotify",
    "music",
    "electron",
    "react",
    "redux",
    "tailwindcss",
    "tailwind-scrollbar",
    "framer-motion",
    "phosphor-icons",
    "nextui",
    "dj",
    "library",
    "playlist",
    "rekordbox"
  ],
  "dependencies": {
    "@nextui-org/react": "^2.4.0",
    "@phosphor-icons/react": "^2.0.15",
    "@react-stately/data": "^3.11.2",
    "atropos": "^2.0.2",
    "cors": "^2.8.5",
    "electron-is-dev": "^3.0.1",
    "express": "^4.19.2",
    "fast-xml-parser": "^4.3.6",
    "framer-motion": "^11.2.9",
    "he": "^1.2.0",
    "id3js": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.22.3",
    "react-scripts": "^5.0.1",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "spotify-web-api-js": "^1.5.2",
    "spotify-web-api-node": "^5.0.2",
    "style-loader": "^4.0.0"
  },
  "type": "module",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-prod": "npm run build && electron .",
    "package": "npm run build && electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "electron": "^30.0.9",
    "electron-builder": "^24.13.3",
    "electron-react-devtools": "^0.5.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.2.5",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.3",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.l3-n0x.trackmatch",
    "productName": "TrackMatch",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "src/assets",
        "to": "assets",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "icon": "build/icon.ico"
    }
  }
}
